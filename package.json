{
  "name": "cypress-cucumber-exercise1",
  "version": "1.0.0",
  "description": "Implementation of exercise 1 using Cypress with Cucumber",
  "main": "index.js",
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "dependencies": {
    "cypress": "^14.0.2",
    "cypress-cucumber-preprocessor": "^4.3.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/e2e/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "devDependencies": {
    "cypress-screenplay": "^0.5.67"
  }
}
